    })], 8 /* PROPS */, _hoisted_14)])) : _createCommentVNode(\"v-if\", true)], 12 /* STYLE, PROPS */, _hoisted_5);\n  }), 128 /* KEYED_FRAGMENT */)), _createElementVNode(\"tr\", null, [_createElementVNode(\"td\", _hoisted_15, [_createElementVNode(\"button\", {\n    onClick: _cache[0] || (_cache[0] = $event => $options.selectAll()),\n    style: _normalizeStyle($options.selectAllBtnStyle()),\n    id: \"selectAllBtn\",\n    class: \"roundBtn\"\n  }, null, 4 /* STYLE */)]), _cache[7] || (_cache[7] = _createElementVNode(\"td\", {\n    id: \"selectAllText\"\n  }, \"全选\", -1 /* HOISTED */)), _createElementVNode(\"td\", _hoisted_16, \" 总价 ：$ \" + _toDisplayString($data.subTotal), 1 /* TEXT */), _createElementVNode(\"td\", _hoisted_17, [_createElementVNode(\"button\", {\n    onClick: _cache[1] || (_cache[1] = $event => $options.proceed()),\n    id: \"btnProceed\"\n  }, [_createVNode(_component_FontAwesomeIcon, {\n    icon: \"clipboard-list\"\n  }), _cache[5] || (_cache[5] = _createTextVNode(\" 提交订单 \"))]), _createElementVNode(\"button\", {\n    onClick: _cache[2] || (_cache[2] = $event => $options.removeSelected()),\n    id: \"btnRemoveSelected\"\n  }, [_createVNode(_component_FontAwesomeIcon, {\n    icon: \"trash\"\n  }), _cache[6] || (_cache[6] = _createTextVNode(\" 删除选中 \"))])])])])])])]);\n}","map":{"version":3,"names":["class","key","colspan","id","_createElementBlock","_hoisted_1","_createElementVNode","_hoisted_2","_createTextVNode","_createVNode","_component_FontAwesomeIcon","icon","_toDisplayString","$data","cart","length","_hoisted_3","style","_createCommentVNode","_hoisted_4","_cache","_Fragment","_renderList","cartitem","itemid","_normalizeStyle","$options","rowStyle","isInTrash","onClick","$event","select","selectBtnStyle","_hoisted_6","productid","description","instock","subtract","_hoisted_7","type","quantity","onKeyup","calculate","updateCartItemQuantity","_hoisted_8","add","_hoisted_9","listprice","_hoisted_10","toBeRemove","_hoisted_11","_hoisted_12","addToCart","_hoisted_13","remove","_hoisted_14","_hoisted_5","_hoisted_15","selectAll","selectAllBtnStyle","_hoisted_16","subTotal","_hoisted_17","proceed","removeSelected"],"sources":["C:\\Users\\13920\\Downloads\\ournewpetstore\\src\\components\\ShoppingCart.vue"],"sourcesContent":["<template>\r\n  <div class=\"cart-container\">\r\n    <div class=\"cart-form\">\r\n      <h2>\r\n        Shopping Cart \r\n        <FontAwesomeIcon icon=\"cart-shopping\" /> {{ cart.length }}\r\n      </h2>\r\n      \r\n      <table class=\"cart\">\r\n        <thead>\r\n          <th style=\"width: 48px;\"></th>\r\n          <th>Item ID</th>\r\n          <th>Product ID</th>\r\n          <th>Description</th>\r\n          <th>In Stock</th>\r\n          <th>Quantity</th>\r\n          <th>List Price</th>\r\n          <th>Total Cost</th>\r\n          <th>Option</th>\r\n        </thead>\r\n\r\n        <tbody> \r\n          <!-- 空购物车 -->\r\n          <td style=\"background-color: #fff;\"></td>\r\n          <div v-if=\"cart.length === 0\" class=\"empty-cart\" colspan=\"6\">\r\n            <p class=\"empty\">您的购物车是空的</p>\r\n          </div>\r\n          \r\n          <tr v-for=\"cartitem in cart\" :key=\"cartitem.itemid\" :id=\"`row-${cartitem.itemid}`\" :style=\"rowStyle(cartitem)\">\r\n            <td>\r\n              <button v-if=\"!cartitem.isInTrash\" @click=\"select(cartitem.itemid)\" \r\n                :id=\"`select-${cartitem.itemid}`\" class=\"roundBtn selectbtn\"\r\n                :style=\"selectBtnStyle(cartitem)\"></button>\r\n            </td>\r\n            <td>{{ cartitem.itemid }}</td>\r\n            <td>{{ cartitem.productid }}</td>\r\n            <td>{{ cartitem.description }}</td>\r\n            <td>{{ cartitem.instock }}</td>\r\n            <td>\r\n              <button @click=\"subtract(cartitem.itemid)\" class=\"roundBtn\">\r\n                <FontAwesomeIcon icon=\"minus\" /> \r\n              </button>\r\n              <input type=\"text\" v-model=\"cartitem.quantity\" @keyup=\"calculate,updateCartItemQuantity(cartitem.itemid,cartitem.quantity)\">\r\n              <button @click=\"add(cartitem.itemid)\" class=\"roundBtn\">\r\n                <FontAwesomeIcon icon=\"plus\" /> \r\n              </button>\r\n            </td>\r\n            <td>${{ cartitem.listprice }}</td>\r\n            <td>${{ cartitem.listprice * cartitem.quantity }}</td>\r\n            <td v-if=\"!cartitem.isInTrash\">\r\n              <button @click=\"toBeRemove(cartitem.itemid)\" :id=\"`btn-${cartitem.itemid}`\" \r\n                class=\"removeBtn roundBtn\">\r\n                <FontAwesomeIcon icon=\"minus\" /> \r\n              </button>\r\n            </td>\r\n            <td v-if=\"cartitem.isInTrash\">\r\n              <button @click=\"addToCart(cartitem.itemid)\" :id=\"`btn-${cartitem.itemid}`\" \r\n                class=\"removeBtn roundBtn\">\r\n                <FontAwesomeIcon icon=\"cart-plus\" /> \r\n              </button>\r\n              <button @click=\"remove(cartitem.itemid)\" :id=\"`btn-${cartitem.itemid}`\" \r\n                class=\"removeBtn roundBtn\">\r\n                <FontAwesomeIcon icon=\"trash\" /> \r\n              </button>\r\n            </td>\r\n          </tr>\r\n          \r\n          <tr>\r\n            <td colspan=\"1\">\r\n              <button @click=\"selectAll()\" :style=\"selectAllBtnStyle()\"\r\n                id=\"selectAllBtn\" class=\"roundBtn\">\r\n              </button>\r\n            </td>\r\n            <td id=\"selectAllText\">全选</td>\r\n            <td colspan=\"2\" id=\"subTotal\"> 总价 ：$ {{ subTotal }}</td>\r\n            <td colspan=\"5\">\r\n              <button @click=\"proceed()\" id=\"btnProceed\">\r\n                <FontAwesomeIcon icon=\"clipboard-list\" /> \r\n                提交订单\r\n              </button>\r\n              <button @click=\"removeSelected()\" id=\"btnRemoveSelected\">\r\n                <FontAwesomeIcon icon=\"trash\" /> \r\n                删除选中\r\n              </button>\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nconst API_URL = 'http://localhost:8090/api/cart';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      subTotal: 0,\r\n      isAllSelected: false,\r\n      cart: [],\r\n      cartService: {\r\n        // 获取商品列表\r\n        async getCartItems() {\r\n          return axios.get(API_URL).then(response => response.data);\r\n        },\r\n        // 更新购物车某商品数量\r\n        async updateCartItem(id, quantity) {\r\n          return axios.put(`${API_URL}/items/${id}`, null, {\r\n            params: {\r\n              quantity: quantity,\r\n              username: \"j2ee\"\r\n            }\r\n          }).then(response => response.data);\r\n        },\r\n        // 删除某商品\r\n        async removeFromCart(id) {\r\n          return axios.delete(`${API_URL}/items/${id}`, {\r\n            params: {\r\n              itemId: id,\r\n              username: \"j2ee\"\r\n            }\r\n          });\r\n        },\r\n        async proceedToCheckout() {\r\n          // 跳转至订单页面\r\n        }\r\n      }\r\n    }\r\n  },\r\n  \r\n  mounted() {\r\n    this.loadCartData();\r\n  },\r\n  \r\n  methods: {\r\n    // 数据加载方法\r\n    async loadCartData() {\r\n      try {\r\n        const response = await this.cartService.getCartItems();\r\n        response.data.items.forEach(element => {\r\n          console.log(element)\r\n          let cartitem = {\r\n            itemid: element.cart.itemId,\r\n            productid: element.cart.productId,\r\n            description: element.cart.description,\r\n            instock: element.cart.in_stock,\r\n            quantity: element.cart.quantity,\r\n            listprice: this.createPrice(element.cart.listPrice),\r\n            isSelected: false,\r\n            isInTrash: false\r\n          }\r\n          this.cart.push(cartitem)\r\n        });\r\n      } catch (error) {\r\n        console.error('加载购物车数据失败:', error);\r\n      }\r\n    },\r\n    \r\n    // 商品操作方法\r\n    async updateCartItemQuantity(itemid, quantity) {\r\n      try {\r\n        await this.cartService.updateCartItem(itemid, quantity);\r\n      } catch (error) {\r\n        console.error('更新数量失败:', error);\r\n      }\r\n    },\r\n    \r\n    async remove(itemid) {\r\n      this.cart.forEach(async item => {\r\n        if (item.itemid === itemid) { \r\n          this.cart.splice(this.cart.indexOf(item), 1);\r\n          await this.cartService.removeFromCart(itemid)\r\n        }\r\n      });\r\n    },\r\n    \r\n    // 数量操作方法\r\n    async subtract(itemid) {\r\n      this.cart.forEach(async item => {\r\n        if (item.itemid === itemid) { \r\n          item.quantity--;\r\n          await this.updateCartItemQuantity(itemid, item.quantity);\r\n        }\r\n      });\r\n      this.calculate(); \r\n    },\r\n    \r\n    async add(itemid) {\r\n      this.cart.forEach(async item => {\r\n        if (item.itemid === itemid) { \r\n          item.quantity++;\r\n          await this.updateCartItemQuantity(itemid, item.quantity);\r\n        }\r\n      });\r\n      this.calculate(); \r\n    },\r\n    \r\n    // 选择操作方法\r\n    select(itemid) {\r\n      this.cart.forEach(async item => {\r\n        if (item.itemid === itemid) {\r\n          if (item.isSelected) {\r\n            item.isSelected = false\r\n          } else {\r\n            item.isSelected = true\r\n          }\r\n          await this.updateCartItemQuantity(itemid, item.quantity);\r\n        }\r\n      });\r\n      this.calculate(); \r\n      this.isAllSelect();\r\n    },\r\n    \r\n    isAllSelect() {\r\n      if (this.cart.some(item => !item.isInTrash && !item.isSelected)) {\r\n        this.isAllSelected = false\r\n        return;\r\n      }\r\n      this.selectAll()\r\n    },\r\n    \r\n    selectAll() {\r\n      if (!this.isAllSelected) {\r\n        this.cart.forEach(item => {\r\n          if (!item.isInTrash) {\r\n            item.isSelected = true \r\n          }\r\n        });\r\n        this.isAllSelected = true\r\n      } else {\r\n        this.cart.forEach(item => {\r\n          item.isSelected = false\r\n        });\r\n        this.isAllSelected = false\r\n      }\r\n      this.calculate()\r\n    },\r\n    \r\n    // 计算和价格方法\r\n    calculate() {\r\n      this.subTotal = 0\r\n      this.cart.forEach(async item => {\r\n        if (item.isSelected) {\r\n          this.subTotal += item.quantity * item.listprice\r\n        }\r\n      });  \r\n    },\r\n    \r\n    isTwoDecimal(num) {\r\n      return /^-?\\d+(\\.\\d{1,2})?$/.test(num.toString());\r\n    },\r\n    \r\n    createPrice(value) {\r\n      if (!this.isTwoDecimal(value)) {\r\n        throw new Error('价格必须保留至多两位小数');\r\n      }\r\n      return parseFloat(value.toFixed(2));\r\n    },\r\n    \r\n    // 其他操作方法\r\n    toBeRemove(itemid) {\r\n      this.cart.forEach(async item => {\r\n        if (item.itemid === itemid) { \r\n          this.cart.push(item)\r\n          this.cart.splice(this.cart.indexOf(item), 1);\r\n          this.cart[this.cart.length - 1].isInTrash = true;\r\n          this.cart[this.cart.length - 1].isSelected = false;\r\n          this.calculate()\r\n          return;\r\n        }\r\n      });\r\n    },\r\n    \r\n    addToCart(itemid) {\r\n      this.cart.forEach(async item => {\r\n        if (item.itemid === itemid) { \r\n          this.cart.unshift(item)\r\n          this.cart.splice(this.cart.indexOf(item)+1, 1);\r\n          this.cart[0].isInTrash = false;\r\n        }\r\n      });\r\n      this.isAllSelected = false\r\n    },\r\n    \r\n    proceed() {\r\n      // 提交订单\r\n    },\r\n    \r\n    async removeSelected() {\r\n      if (confirm(\"确认删除所选中的商品？\")) { \r\n        this.subTotal = 0\r\n        this.isAllSelected = false\r\n        let i = 0\r\n        while (i < this.cart.length) {\r\n          if (this.cart[i].isSelected) {\r\n            this.remove(this.cart[i].itemid)\r\n          } else {\r\n            i++\r\n          }\r\n        }\r\n      }\r\n    },\r\n    \r\n    // 样式方法\r\n    rowStyle(item) {\r\n      return {\r\n        backgroundColor: item.isInTrash ? '#ccc' : ''\r\n      }\r\n    },\r\n    \r\n    selectAllBtnStyle() {\r\n      return {\r\n        backgroundColor: this.isAllSelected ? 'rgb(22,92,145,0.2)' : '#fff'\r\n      }\r\n    },\r\n    \r\n    selectBtnStyle(item) {\r\n      return {\r\n        backgroundColor: item.isSelected ? 'rgb(22,92,145,0.2)' : '#fff'\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.cart-container {\r\n  margin: 0 auto;\r\n  width: 100%;\r\n  padding: 30px;\r\n}\r\n\r\n.cart-form {\r\n  width: 100%;\r\n  margin: 0 auto;\r\n  padding: 30px;\r\n  border-radius: 10px;\r\n  background-color: rgba(255, 255, 255, 0.9);\r\n  box-shadow: 0 5px 15px rgba(0, 0, 0, 0.1);\r\n  transform: translateY(0);\r\n  transition: all 0.3s ease;\r\n  position: relative;\r\n  z-index: 1;\r\n}\r\n\r\n.cart-form:hover {\r\n  transform: translateY(-5px);\r\n  box-shadow: 0 8px 20px rgba(0, 0, 0, 0.15);\r\n}\r\n\r\nh2 {\r\n  width: 100%;\r\n  text-align: center;\r\n  word-spacing: 5px;\r\n  margin: 0;\r\n  height: 60px;\r\n  line-height: 0px;\r\n  color: rgb(22,92,145);\r\n  text-shadow: 0 0 5px #fff; \r\n  font-size: 40px;\r\n}\r\n\r\n.cart {\r\n  color: rgb(22,92,145);\r\n  text-align: center;\r\n  margin: 0 auto;\r\n  width: 100%;\r\n  border-radius: 10px;\r\n}\r\n\r\nth {\r\n  background-color: rgba(22, 92, 145, 0.4);\r\n  font-size: 24px;\r\n  text-shadow: 0 0 5px #fff; \r\n  font-style: oblique;\r\n}\r\n\r\ntr {\r\n  background-color: rgba(22, 92, 145, 0.1);\r\n  font-size: 20px;\r\n}\r\n\r\ntd {\r\n  padding-inline: 5px;\r\n  width: fit-content;\r\n  text-shadow: 0 0 5px rgba(22, 92, 145, 0.2); \r\n}\r\n\r\ninput {\r\n  height: 35px;\r\n  font-size: 20px;\r\n  margin: 2px 0px;\r\n  width: 40px;\r\n  padding-left: 10px;\r\n  border-radius: 10px;\r\n}\r\n\r\n.roundBtn {\r\n  font-size: 20px;\r\n  padding: 0%;\r\n  height: 36px;