>\r\n        <tr>\r\n          <td>Country</td>\r\n          <td><input type=\"text\" v-model=\"formData.billCountry\" class=\"form-control\"></td>\r\n        </tr>\r\n      </table>\r\n      <button type=\"submit\" class=\"btn btn-primary\">Continue to Review</button>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      formData: {\r\n        cardType: '',\r\n        creditCard: '',\r\n        expiryDate: '',\r\n        billToFirstName: '',\r\n        billToLastName: '',\r\n        billAddress1: '',\r\n        billAddress2: '',\r\n        billCity: '',\r\n        billState: '',\r\n        billZip: '',\r\n        billCountry: ''\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    axios.get('http://localhost:9090/orders')\r\n      .then(response => {\r\n        const data = response.data.data;\r\n        Object.assign(this.formData, {\r\n          cardType: data.cardType,\r\n          creditCard: data.creditCard,\r\n          expiryDate: data.expiryDate,\r\n          billToFirstName: data.billToFirstName,\r\n          billToLastName: data.billToLastName,\r\n          billAddress1: data.billAddress1,\r\n          billAddress2: data.billAddress2,\r\n          billCity: data.billCity,\r\n          billState: data.billState,\r\n          billZip: data.billZip,\r\n          billCountry: data.billCountry\r\n        });\r\n      })\r\n      .catch(error => console.error(error));\r\n  },\r\n  methods: {\r\n    submitOrder() {\r\n      axios.post('http://localhost:9090/orders', this.formData)\r\n        .then(response => {\r\n          this.$router.push('/confirm');\r\n        })\r\n        .catch(error => console.error(error));\r\n    }\r\n  }\r\n}\r\n</script>\r\n  \r\n  <style scoped>\r\n  .payment-container {\r\n    max-width: 800px;\r\n    margin: 0 auto;\r\n    padding: 20px;\r\n  }\r\n  \r\n  .detail-table {\r\n    width: 100%;\r\n    border-collapse: collapse;\r\n    marg